{
  "manifest_version": 2,

  "name": "QR Code",
  "description": "Generate QR Code for web page.",
  "version": "1.0",

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html",
    "default_title": "Generate QRCode"
  },
  "permissions": [
    "activeTab",
    "https://ajax.googleapis.com/"
  ]
}